{
    "name": "Preinstall Extensions",
    "id": "preinstall-extensions",
    "version": "0.0.2",
    "description": "Preinstall VSCode Extensions",
    "options": {
        "extensions": {
            "type": "string",
            "proposals": [
                "github.vscode-github-actions",
                "ms-azuretools.vscode-docker",
                "ms-python.python",
                "ms-vscode.powershell"
            ],
            "default": "",
